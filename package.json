{
  "name": "@anmiles/prototypes",
  "version": "3.0.0",
  "description": "Provides prototype extensions for native JS modules",
  "keywords": [
    "prototype",
    "extension",
    "native"
  ],
  "author": "Anatoliy Oblaukhov",
  "homepage": "https://gitlab.com/anmiles/lib/prototypes",
  "repository": "gitlab:anmiles/lib/prototypes",
  "license": "MIT",
  "engines": {
    "node": ">=18.14.2"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --verbose",
    "test:coverage": "npm test -- --coverage",
    "test:ci": "npm test -- --ci --coverage",
    "test:watch": "npm test -- --watch",
    "test:watch:coverage": "npm test -- --watch --coverage",
    "test:report:coverage": "nyc report --nycrc-path ./coverage.config.js -t ./coverage --report-dir ./coverage"
  },
  "dependencies": {
    "iconv-lite": "^0.6.3"
  },
  "devDependencies": {
    "@anmiles/eslint-config": "^2.0.0",
    "@types/event-emitter": "^0.3.3",
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-plugin-align-assignments": "^1.1.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.2.3",
    "event-emitter": "^0.3.5",
    "jest": "^29.7.0",
    "nyc": "^15.1.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
